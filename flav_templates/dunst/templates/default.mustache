[base16_low]
	msg_urgency		= low
	background		= "#{{base00-hex}}"
	foreground		= "#{{base0B-hex}}"
	frame_color 	= "#{{base0B-hex}}"
	separator_color	= "#{{base0B-hex}}"

[base16_normal]
	msg_urgency		= normal
	background		= "#{{base00-hex}}"
	foreground		= "#{{base0A-hex}}"
	frame_color 	= "#{{base0A-hex}}"
	separator_color	= "#{{base0A-hex}}"

[base16_critical]
	msg_urgency		= critical
	background		= "#{{base08-hex}}"
	foreground		= "#{{base00-hex}}"
	frame_color 	= "#{{base08-hex}}"
	separator_color	= "#{{base08-hex}}"
